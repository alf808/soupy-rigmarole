Host team1-acme-developers
     Hostname xx.xx.xx.xx
     User ec2-user
     Port 22
     ForwardAgent yes
     IdentityFile ~/.ssh/team1-acme-developers.pem

Host private-dev
     User ec2-user
     ProxyCommand ssh team1-acme-developers nc xx.xx.xx.xx 22
     ForwardAgent yes
     IdentityFile ~/.ssh/team1-acme-developers.pem

